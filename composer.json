{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "fanzo/service-common": "dev-main",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "bennett-treptow/laravel-migration-generator": "^4.3",
        "fanzo/php-coding-standards": "dev-main",
        "laravel/telescope": "^4.10"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../service-common"
        },
        {
            "type": "path",
            "url": "../php-coding-standards"
        },
        {
            "url": "https://github.com/MatchPint/batch.git",
            "type": "git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=common",
            "cp -R ./vendor/fanzo/php-coding-standards/git-hooks/prepare-commit-msg .git/hooks/prepare-commit-msg && chmod u+x .git/hooks/prepare-commit-msg",
            "cp -R ./vendor/fanzo/php-coding-standards/git-hooks/pre-commit-laravel .git/hooks/pre-commit && chmod u+x .git/hooks/pre-commit",
            "[ $COMPOSER_DEV_MODE -eq 0 ] || php artisan vendor:publish --tag=telescope-assets"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
